{
  "name": "crossword-maker",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://cellule.github.io/crossword-maker/",
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "codegen:watch": "graphql-codegen -w",
    "codegen": "graphql-codegen",
    "dev": "vite",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "lint:fix": "DEBUG=eslint.eslint eslint . --fix",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@apollo/client": "^3.11.8",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@graphql-tools/schema": "^10.0.7",
    "@mui/icons-material": "^6.1.5",
    "@mui/material": "^6.1.5",
    "graphql": "^16.9.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^6.27.0",
    "react-router-dom": "^6.27.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/client-preset": "^4.5.0",
    "@graphql-codegen/typescript": "^4.1.1",
    "@graphql-codegen/typescript-operations": "^4.3.1",
    "@graphql-codegen/typescript-react-apollo": "^4.3.2",
    "@graphql-codegen/typescript-resolvers": "^4.4.0",
    "@parcel/watcher": "^2.4.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "prettier": "^3.3.3",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10"
  },
  "packageManager": "yarn@4.5.1"
}
